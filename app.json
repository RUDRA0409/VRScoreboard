    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier":"com.Rudrascoretrackerapp"
    },
    "android": {
  
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.Rudrascoretrackerapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
